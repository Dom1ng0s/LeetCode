#define INT_MAX 2147483647
#define INT_MIN (-2147483648)

long long int myAtoi(const char* s) {
    int i = 0;
    long long int r = 0;
    int sign = 1;
    while (s[i] == ' ') i++;
    if (s[i] == '-') {
        sign = -1;
        i++;
    } else if (s[i] == '+') {
        i++;
    }
    while (s[i] >= '0' && s[i] <= '9') {
        r = r * 10 + (s[i] - '0');
        if (r * sign > INT_MAX) return INT_MAX;
        if (r * sign < INT_MIN) return INT_MIN;
        i++;
    }
    return r * sign;
}
