#include <stdio.h>
#include <stdlib.h>

int main ()
{
    int n;
    scanf("%d",&n);
    if(n < 3)
    {
        
        return -1;
    }

    while (n % 3 != 0)
    {
        if(n < 3)
        {
            
            return -1;
        }
        n = n/10;
        if (n == 0)
        {
            return -1;
        }
    }
    return n;
    
}
